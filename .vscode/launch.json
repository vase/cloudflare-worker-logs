  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: Run",
      "request": "launch",
      "type": "pwa-node",
      "program": "main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--unstable",
        "--allow-all"
      ],
      "env": {
        "CF_API_TOKEN": "",
        "CF_ACCOUNT_ID": "",
        "LOGGING_MONGO_URI": "mongodb://127.0.0.1:27017"
      },
      "attachSimplePort": 9229
    }
  ]
}
